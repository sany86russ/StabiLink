{
  "profiles": [
    {
      "name": "FAKE_FAKEDSPLIT",
      "description": "FakeTLS + fakedsplit (универсальный)",
      "filter": "tcp.DstPort == 443",
      "options": "--wf-tcp=443 --ipset=ipset.txt --dpi-desync=fake,fakedsplit",
      "strategy": "Default",
      "timeout": 7,
      "asn_hint": ["MTS_GROUP", "ROSTELECOM", "BEELINE", "MGTS"]
    },
    {
      "name": "FAKE_IPFRAG2",
      "description": "FakeTLS + ipfrag2 (лучше для YouTube/Instagram)",
      "filter": "tcp.DstPort == 443",
      "options": "--wf-tcp=443 --ipset=ipset.txt --dpi-desync=fake,ipfrag2",
      "strategy": "Fragmentation",
      "timeout": 7,
      "asn_hint": ["GOOGLE", "META"]
    },
    {
      "name": "FAKE_UDPLEN",
      "description": "FakeTLS + udplen (для ChatGPT/Twitter)",
      "filter": "tcp.DstPort == 443",
      "options": "--wf-tcp=443 --ipset=ipset.txt --dpi-desync=fake,udplen --dpi-desync-udplen-increment=3",
      "strategy": "Tamper",
      "timeout": 7,
      "asn_hint": ["MICROSOFT", "AZURE"]
    },
    {
      "name": "QUIC_BLOCK",
      "description": "Блокировка QUIC (HTTP/3 → TCP)",
      "filter": "udp.DstPort == 443",
      "options": "--wf-udp=443 --dpi-desync=fake,udplen --dpi-desync-udplen-increment=1",
      "strategy": "QuicStrategy",
      "timeout": 5,
      "asn_hint": ["ALL"]
    },
    {
      "name": "DISCORD_VOICE",
      "description": "Discord Voice UDP (голосовые чаты)",
      "filter": "udp.DstPort >= 50000 and udp.DstPort <= 65000",
      "options": "--wf-udp=50000-65000 --dpi-desync=udplen --dpi-desync-udplen-increment=2",
      "strategy": "VoiceStrategy",
      "timeout": 10,
      "asn_hint": ["DISCORD"]
    }
  ]
}
