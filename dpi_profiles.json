{
  "profiles": [
    {
      "name": "FAKE_TLS_AUTO",
      "description": "Fake TLS Auto: универсальная подмена TLS ClientHello",
      "filter": "tcp.DstPort == 443",
      "options": "--sni --fake",
      "strategy": "FakeTlsStrategy",
      "timeout": 5,
      "asn_hint": ["ROSTELECOM", "BEELINE", "YOTA", "MTS_GROUP"]
    },
    {
      "name": "FAKE_TLS_ALT",
      "description": "Fake TLS ALT: FakeTLS + Host обфускация",
      "filter": "tcp.DstPort == 443",
      "options": "--sni --host --fake",
      "strategy": "FakeTlsStrategy",
      "timeout": 5,
      "asn_hint": ["BEELINE", "TELE2", "MTS_GROUP"]
    },
    {
      "name": "FAKE_TLS_ALT2",
      "description": "Fake TLS ALT2: FakeTLS + Split Handshake",
      "filter": "tcp.DstPort == 443",
      "options": "--sni --split --fake",
      "strategy": "FakeTlsStrategy",
      "timeout": 6,
      "asn_hint": ["ROSTELECOM", "MTS_GROUP"]
    },
    {
      "name": "ALT",
      "description": "ALT: стандартная SNI фрагментация",
      "filter": "tcp.DstPort == 443",
      "options": "--sni --split",
      "strategy": "FragmentationStrategy",
      "timeout": 5,
      "asn_hint": ["ROSTELECOM", "MTS_GROUP"]
    },
    {
      "name": "ALT2",
      "description": "ALT2: фрагментация с MSS 1200",
      "filter": "tcp.DstPort == 443",
      "options": "--sni --split --mss 1200",
      "strategy": "FragmentationStrategy",
      "timeout": 5,
      "asn_hint": ["MTS_GROUP", "ROSTELECOM"]
    },
    {
      "name": "ALT3",
      "description": "ALT3: агрессивная фрагментация (MSS 800)",
      "filter": "tcp.DstPort == 443",
      "options": "--sni --split --mss 800",
      "strategy": "FragmentationStrategy",
      "timeout": 6,
      "asn_hint": ["BEELINE", "TELE2", "MTS_GROUP"]
    },
    {
      "name": "ALT4",
      "description": "ALT4: фрагментация с MSS 600",
      "filter": "tcp.DstPort == 443",
      "options": "--sni --split --mss 600",
      "strategy": "FragmentationStrategy",
      "timeout": 6,
      "asn_hint": ["YOTA", "MTS_GROUP"]
    },
    {
      "name": "ALT5",
      "description": "ALT5: фрагментация + Host",
      "filter": "tcp.DstPort == 443",
      "options": "--sni --host --split",
      "strategy": "FragmentationStrategy",
      "timeout": 6,
      "asn_hint": ["ROSTELECOM", "MTS_GROUP"]
    },
    {
      "name": "DNS_FAKE_TLS",
      "description": "DoH + FakeTLS: DNS-over-HTTPS + FakeTLS",
      "filter": "udp.DstPort == 53 or tcp.DstPort == 443",
      "options": "--dns-redirect 1.1.1.1 --sni --fake",
      "strategy": "MixedStrategy",
      "timeout": 8,
      "asn_hint": ["BEELINE", "TELE2", "ROSTELECOM", "MTS_GROUP"]
    },
    {
      "name": "QUIC_SPLITTER",
      "description": "QUIC Splitter: HTTP/3 Initial fragmentation",
      "filter": "udp.DstPort == 443",
      "options": "--quic-frag",
      "strategy": "QuicStrategy",
      "timeout": 8,
      "asn_hint": ["YOTA", "ROSTELECOM", "BEELINE", "MTS_GROUP"]
    },
    {
      "name": "MTTC",
      "description": "MTTC: спец. правила для МТС и МГТС",
      "filter": "tcp.DstPort == 443",
      "options": "--sni --split --mss 1400",
      "strategy": "MttcStrategy",
      "timeout": 10,
      "asn_hint": ["MTS_GROUP"]
    },

    {
      "name": "MTTC2",
      "description": "MTTC2: спец. правила для региональных провайдеров (МТС/МГТС)",
      "filter": "tcp.DstPort == 443",
      "options": "--sni --host --mss 1300",
      "strategy": "MttcStrategy",
      "timeout": 10,
      "asn_hint": ["MTS_GROUP", "ER-TELECOM"]
    },
    {
      "name": "MTTC3",
      "description": "MTTC3: альтернативные правила для MGTS",
      "filter": "tcp.DstPort == 443",
      "options": "--sni --split --mss 1000",
      "strategy": "MttcStrategy",
      "timeout": 10,
      "asn_hint": ["MTS_GROUP"]
     },
     {
      "name": "MTTC4",
      "description": "MTTC4: MGTS fallback",
      "filter": "tcp.DstPort == 443",
      "options": "--sni --host --split --mss 900",
      "strategy": "MttcStrategy",
      "timeout": 12,
      "asn_hint": ["MTS_GROUP"]
     }
  ]
}
