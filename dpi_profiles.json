      "filter": "tcp.DstPort == 443",
      "options": "--sni --split --mss 1400",
      "strategy": "MttcStrategy",
      "timeout": 10,
      "asn_hint": ["MTS_GROUP"]
    },

    {
      "name": "MTTC2",
      "description": "MTTC2: спец. правила для региональных провайдеров (МТС/МГТС)",
      "filter": "tcp.DstPort == 443",
      "options": "--sni --host --mss 1300",
      "strategy": "MttcStrategy",
      "timeout": 10,
      "asn_hint": ["MTS_GROUP", "ER-TELECOM"]
    },
    {
      "name": "MTTC3",
      "description": "MTTC3: альтернативные правила для MGTS",
      "filter": "tcp.DstPort == 443",
      "options": "--sni --split --mss 1000",
      "strategy": "MttcStrategy",
      "timeout": 10,
      "asn_hint": ["MTS_GROUP"]
     },
     {
      "name": "MTTC4",
      "description": "MTTC4: MGTS fallback",
      "filter": "tcp.DstPort == 443",
      "options": "--sni --host --split --mss 900",
      "strategy": "MttcStrategy",
      "timeout": 12,
      "asn_hint": ["MTS_GROUP"]
     },
{
  "name": "DNS_FAKE_TLS3",
  "description": "DoH с Quad9",
  "filter": "udp.DstPort == 53 or tcp.DstPort == 443",
  "options": "--dns-redirect 9.9.9.9 --sni --fake",
  "strategy": "MixedStrategy",
  "timeout": 8,
  "asn_hint": ["MTS_GROUP"]
},
{
  "name": "DNS_DOH",
  "description": "DNS-over-HTTPS обход MGTS",
  "filter": "udp.DstPort == 53 or tcp.DstPort == 443",
  "options": "--doh https://dns.google/dns-query --sni --fake",
  "strategy": "MixedStrategy",
  "timeout": 10,
  "asn_hint": ["MTS_GROUP"]
},
    {
      "name": "QUIC_SPLITTER",
      "description": "QUIC Splitter (HTTP/3 workaround)",
      "filter": "udp.DstPort == 443",
      "options": "--quic-frag --no-quic",
      "strategy": "QuicStrategy",
      "timeout": 9,
      "asn_hint": ["MTS_GROUP", "BEELINE", "YOTA"]
    },
{
  "name": "HYBRID_PROXY",
  "description": "DPI + локальный SOCKS5 Proxy",
  "filter": "tcp.DstPort == 443",
  "options": "--sni --split --proxy 127.0.0.1:9050",
  "strategy": "HybridStrategy",
  "timeout": 12,
  "asn_hint": ["MTS_GROUP"]
},
 {
      "name": "DiscordVoice",
      "description": "Discord Voice UDP",
      "filter": "udp.DstPort >= 50000 and udp.DstPort <= 65000",
      "options": "--udp-voice-bypass",
      "strategy": "VoiceStrategy",
      "timeout": 10,
      "asn_hint": ["MTS_GROUP", "ROSTELECOM", "BEELINE"]
    }
  ]
}
