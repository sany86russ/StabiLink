      "options": "--sni --host --split",
      "strategy": "FragmentationStrategy",
      "timeout": 6,
      "asn_hint": ["ROSTELECOM", "MTS_GROUP"]
    },
{
  "name": "ALT6",
  "description": "ALT6: агрессивная фрагментация (MSS 600 + host)",
  "filter": "tcp.DstPort == 443",
  "options": "--sni --host --split --mss 600",
  "strategy": "FragmentationStrategy",
  "timeout": 8,
  "asn_hint": ["MTS_GROUP"]
},
{
  "name": "ALT7",
  "description": "ALT7: fallback для MGTS",
  "filter": "tcp.DstPort == 443",
  "options": "--sni --split --mss 500",
  "strategy": "FragmentationStrategy",
  "timeout": 8,
  "asn_hint": ["MTS_GROUP"]
},
{
  "name": "ALT8",
  "description": "ALT8: максимальная агрессивная фрагментация для MGTS",
  "filter": "tcp.DstPort == 443",
  "options": "--sni --host --split --mss 480",
  "strategy": "FragmentationStrategy",
  "timeout": 12,
  "asn_hint": ["MTS_GROUP"]
},
    {
      "name": "DNS_FAKE_TLS",
      "description": "DoH + FakeTLS: DNS-over-HTTPS + FakeTLS",
      "filter": "udp.DstPort == 53 or tcp.DstPort == 443",
      "options": "--dns-redirect 1.1.1.1 --sni --fake",
      "strategy": "MixedStrategy",
      "timeout": 8,
      "asn_hint": ["BEELINE", "TELE2", "ROSTELECOM", "MTS_GROUP"]
    },
    {
      "name": "DNS_FAKE_TLS2",
      "description": "DoH + FakeTLS для MGTS",
      "filter": "udp.DstPort == 53 or tcp.DstPort == 443",
      "options": "--dns-redirect 8.8.8.8 --sni --fake",
      "strategy": "MixedStrategy",
      "timeout": 8,
      "asn_hint": ["MTS_GROUP"]
    },
    {
      "name": "QUIC_SPLITTER",
      "description": "QUIC Splitter: HTTP/3 Initial fragmentation",
      "filter": "udp.DstPort == 443",
      "options": "--quic-frag",
      "strategy": "QuicStrategy",
      "timeout": 8,
      "asn_hint": ["YOTA", "ROSTELECOM", "BEELINE", "MTS_GROUP"]
    },
    {
      "name": "MTTC",
